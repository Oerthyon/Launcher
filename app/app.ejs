<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8"/>
    <title>Helios Launcher</title>
    <script src="./assets/js/scripts/uicore.js"></script>
    <script src="./assets/js/scripts/uibinder.js"></script>
    <link type="text/css" rel="stylesheet" href="./assets/css/launcher.css">
    <link type="text/css" rel="stylesheet" href="./assets/css/custom.css">
    <style>
        body {
            transition: background-image 1s ease;
            background-image: url('data:image/jpeg;base64,');
            background-size: cover;
            -webkit-user-select: none;
        }

        #main[overlay] {
            filter: blur(3px) contrast(0.9) brightness(1.0);
        }
    </style>
</head>
<body>

<div class="wrap">
    <%- include('frame') %>

    <!--Body content-->
    <div class="content" id="main" style="height: calc(100vh - 22px);">
        <%- include('welcome') %>
        <%- include('login') %>
        <%- include('settings') %>
        <%- include('landing') %>
        <%- include('overlay') %>
    </div>
    <script>
        document.getElementById('settingsBtn').addEventListener('click', () => {
            switchView(getCurrentView(), getCurrentView() === VIEWS.settings ? VIEWS.landing : VIEWS.settings)
        })
    </script>
</div>
</body>
</html>